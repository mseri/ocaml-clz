(package
 (name cuz)
 (synopsis "Gzip compression support for cohttp-lwt using decompress")
 (documentation "https://mseri.github.io/ocaml-cuz")
 (depends
  (ocaml
   (>= 4.08))
  (bigstringaf
   (>= 0.2.0))
  (cohttp-lwt
   (>= 2.5.0))
  (lwt
   (>= 5.3.0))
  (decompress
   (>= 1.4.0))
  (ptime
   (>= 0.8.0))))
